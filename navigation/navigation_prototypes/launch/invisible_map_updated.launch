<?xml version="1.0"?>
<launch>
  <include file="$(find navigation_prototypes)/launch/frames.launch" />

  <!-- invisible map requires a keyboard press in order to switch between calibration and run modes -->
  <node pkg="keyboard" type="keyboard" name="keyboard" />

  <node pkg="navigation_prototypes" type="data_collection.py" name="data_collection" output="screen" />
  <node pkg="rqt_gui" type="rqt_gui" name="rqt_gui" />

</launch>